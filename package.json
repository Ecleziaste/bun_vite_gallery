{
  "name": "bun_vite_gallery",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint bun_vite_gallery --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint  bun_vite_gallery  --fix ./src --ext .jsx,.ts,.tsx",
    "prettier": "prettier \"src/**/*.ts\" \"src/**/*.tsx\"",
    "prettier:write": "prettier --write --no-error-on-unmatched-pattern \"src/*.{ts,tsx}\" \"src/**/*.{ts,tsx}\"",
    "preview": "vite preview",
    "prepare": "husky install",
    "commit": "cz"
  },
  "dependencies": {
    "commitizen": "^4.3.0",
    "dayjs": "^1.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-import-sort": "^0.0.7",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "importSort": {
    ".js, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
